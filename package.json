{
  "name": "bruxism-dashboard",
  "version": "1.0.0",
  "description": "Web dashboard for bruxism interventions and information",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "dev:vercel": "npx --yes vercel@latest dev",
    "debug:user-data": "node --env-file=.env.local scripts/debug-user-data.mjs",
    "normalize:user-data-interventions": "node --env-file=.env.local scripts/normalize-user-data-interventions.mjs",
    "seed:first-party-content": "node --experimental-default-type=module --env-file=.env.local scripts/seed-first-party-content.mjs",
    "sync:ios-canonical-graph": "node --experimental-default-type=module scripts/sync-ios-canonical-graph.mjs",
    "configure:user-sleep-apnea-profile": "node --env-file=.env.local scripts/configure-user-sleep-apnea-profile.mjs",
    "muse:diagnostics-summary": "node scripts/muse-diagnostics-summary.mjs",
    "ios:explore-ui": "node scripts/ios-ui-explorer.mjs",
    "test": "node --experimental-default-type=module --test test/*.test.mjs",
    "test:all": "node --experimental-default-type=module --test test/*.test.mjs",
    "test:defense-scoring": "node --experimental-default-type=module --test test/defenseScoring.test.mjs",
    "test:protocol": "node --experimental-default-type=module --test test/personalizationModel.unit.test.mjs test/personalizationProtocol.test.mjs test/realityFeedback.integration.test.mjs test/realityProtocolStorage.test.mjs test/storageDomains.unit.test.mjs test/storageIo.unit.test.mjs test/storageSync.unit.test.mjs test/defenseScoring.test.mjs",
    "test:coverage": "node --experimental-default-type=module --test --experimental-test-coverage test/*.test.mjs",
    "guard:ios-hotspots": "./scripts/guard-ios-hotspots.sh",
    "debug:graph-clusters": "node --env-file=.env.local scripts/debug-graph-clusters.mjs",
    "debug:foundation-coverage": "node --env-file=.env.local scripts/debug-foundation-coverage.mjs"
  },
  "packageManager": "npm@10",
  "dependencies": {
    "@supabase/supabase-js": "^2.95.3",
    "cors": "^2.8.5",
    "cytoscape": "^3.30.4",
    "cytoscape-elk": "^2.2.0",
    "elkjs": "^0.9.3",
    "express": "^4.18.2",
    "playwright": "^1.58.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@vercel/node": "^3.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vercel": "^33.0.0"
  }
}
